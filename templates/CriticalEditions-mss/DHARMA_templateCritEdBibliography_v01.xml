<TEI xmlns="http://www.tei-c.org/ns/1.0" xml:lang="eng" type="bibliography"> <!-- mettre un @corresp sur l'élément avec un pointer vers le fichier master? -->
    <teiHeader>
        <fileDesc>
            <titleStmt>
                <title>Encoding Template for Bibliography on Critical Editions</title>
                <!--Change the title of the file with a title for the bibliography, you don't see any use of attributes here, but if you need some to add a subtitle for instance, please use @type="main" and @subtype="bibliography"-->
                <!-- Possibility to add the <editor> tag:
        <editor ref="part:ID">
					<forename></forename>
					<surname></surname>
				</editor>
				OR
				<editor ref="part:ID">
					<name></name>
				</editor>
        -->
                <respStmt>
                    <!-- repeat the respStmt when necessary -->
                    <resp>Encoding of the bibliography</resp>
                    <!-- work done in the file, e.g. editor, intellectual authorship of edition ...-->
                    <persName ref="part:jodo"><!-- update the id -->
                        <forename>John</forename><!-- fill in your  names — it is possible to use only <name> instead of <foreame> and <surname> -->
                        <surname>Doe</surname>
                    </persName>
                </respStmt>
            </titleStmt>
            <publicationStmt>
                <authority>DHARMA</authority>
                <pubPlace><!-- fill in here the place or places where the encoding has been done --></pubPlace>
                <idno type="filename">DHARMA_templateCritEdBibliography_v01</idno>
                <!-- Replace by filename following FNC -->
                <availability>
                    <licence target="https://creativecommons.org/licenses/by/4.0/">
