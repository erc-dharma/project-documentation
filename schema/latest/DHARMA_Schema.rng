   <define name="placeName">
      <element name="placeName">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains an absolute or relative place name. [13.2.3. Place Names]</a:documentation>
         <ref name="macro.phraseSeq"/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="dharma-placeName-div-constraint-rule-55">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xs="http://www.w3.org/2001/XMLSchema"
                      context="tei:placeName[@type='region']">
               <sch:assert test="@subtype='district' or @subtype='province' or @subtype='country' or @subtype='undetermined' or @subtype='land' or @subtype='island'">For @type="region", the @subtype should be one of the following
                                    values: @subtype='district' or @subtype='province' or
                                    @subtype='country' or @subtype='undetermined' or @subtype='land'
                                    or @subtype='island'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xs="http://www.w3.org/2001/XMLSchema"
                      context="tei:placeName[@type='settlement']">
               <sch:assert test="@subtype='town' or @subtype='village' or @subtype='hamlet' or @subtype='armyCamp' or @subtype='undetermined' or @subtype='establishment'">For @type="settlement", the @subtype should be one of the
                                    following values: @subtype='town' or @subtype='village' or
                                    @subtype='hamlet' or @subtype='armyCamp' or
                                    @subtype='undetermined' or
                                @subtype='establishment'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xs="http://www.w3.org/2001/XMLSchema"
                      context="tei:placeName[@type='builtPlace']">
               <sch:assert test="@subtype='temple' or @subtype='monastery' or @subtype='feedingHall' or @subtype='pavillion' or @subtype='undetermined'">For @type="builtPlace", the @subtype should be one of the
                                    following values: @subtype='temple' or @subtype='monastery' or
                                    @subtype='feedingHall' or @subtype='pavillion' or
                                    @subtype='undetermined'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xs="http://www.w3.org/2001/XMLSchema"
                      context="tei:placeName[@type='cultivation']">
               <sch:assert test="@subtype='garden' or @subtype='field' or @subtype='fieldPaddy' or @subtype='fieldMillet' or @subtype='orchard' or @subtype='orchardAreca' or @subtype='orchardCitrus' or @subtype='fieldIrrigated' or @subtype='fieldDry' or @subtype='fieldSalted' or @subtype='undetermined' or @subtype='nursery'">For @type="cultivation", the @subtype should be one of the
                                    following values: @subtype='garden' or @subtype='field' or
                                    @subtype='fieldPaddy' or @subtype='fieldMillet' or
                                    @subtype='orchard' or @subtype='orchardAreca' or
                                    @subtype='orchardCitrus' or @subtype='fieldIrrigated' or
                                    @subtype='fieldDry' or @subtype='fieldSalted' or
                                    @subtype='undetermined' or
                                @subtype='nursery'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xs="http://www.w3.org/2001/XMLSchema"
                      context="tei:placeName[@type='hydrographic']">
               <sch:assert test="@subtype='tank' or @subtype='river' or @subtype='pond' or @subtype='canal' or @subtype='undetermined' or @subtype='wetland' or @sutype='pool' or @subtype='lake'">For @type="hydrographic", the @subtype should be one of the
                                    following values: @subtype='tank' or @subtype='river' or
                                    @subtype='pond' or @subtype='canal' or @subtype='undetermined'
                                    or @subtype='wetland' or @sutype='pool' or
                                    @subtype='lake'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xs="http://www.w3.org/2001/XMLSchema"
                      context="tei:placeName[@type='naturalPlace']">
               <sch:assert test="@subtype='forest' or @subtype='wood' or @subtype='grove' or @subtype='tree' or @subtype='mountain' or @subtype='undetermined' or @subtype='mount'">For @type="naturalPlace", the @subtype should be one of the
                                    following values: @subtype='forest' or @subtype='wood' or
                                    @subtype='grove' or @subtype='tree' or @subtype='mountain' or
                                    @subtype='undetermined' or
                                @subtype='mount'</sch:assert>
            </sch:rule>
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xs="http://www.w3.org/2001/XMLSchema"
                      context="tei:placeName[@type='undetermined']">
                                <sch:assert test="not(@subtype)">@subtype isn't allowed with
                                    @type='undetermined'</sch:assert>
                            </sch:rule>
         </pattern>
         <ref name="att.global.attribute.xmlid"/>
         <ref name="att.global.responsibility.attribute.cert"/>
         <ref name="att.canonical.attribute.key"/>
         <ref name="att.canonical.attribute.ref"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes the element in some sense, using any convenient classification scheme or typology.
Sample values include: 1] region; 2] settlement; 3] builtPlace; 4] cultivation; 5] hydrographic; 6] naturalPlace; 7] undetermined</a:documentation>
               <data type="token">
                  <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})+</param>
               </data>
            </attribute>
         </optional>
         <optional>
            <attribute name="subtype">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">provides a sub-categorization of the element, if needed
Sample values include: 1] province; 2] district; 3] country; 4] town; 5] village; 6] hamlet; 7] armyCamp; 8] temple; 9] monastery; 10] feedingHall; 11] pavillon; 12] garden; 13] field; 14] fieldPaddy; 15] fieldMillet; 16] orchard; 17] orchardAreca; 18] orchardCitrus; 19] tank; 20] river; 21] pond; 22] canal; 23] forest; 24] wood; 25] grove; 26] tree; 27] montain; 28] land; 29] nursery; 30] island; 31] establishment; 32] fieldIrrigated; 33] fieldDry; 34] fieldSalted; 35] wetland; 36] pool; 37] lake; 38] mount; 39] undetermined</a:documentation>
               <data type="token">
                  <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})+</param>
               </data>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="model.rdgPart">
      <notAllowed/>
   </define>
   <define name="app">
      <element name="app">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(apparatus entry) contains one entry in a critical apparatus, with an optional lemma and usually one or more readings or notes on the relevant passage. [12.1.1. The Apparatus Entry]</a:documentation>
         <group>
            <ref name="lem"/>
            <oneOrMore>
               <choice>
                  <ref name="rdg"/>
                  <ref name="note"/>
               </choice>
            </oneOrMore>
         </group>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="dharma-app-one-lem-constraint-rule-62">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xs="http://www.w3.org/2001/XMLSchema"
                      context="tei:app">
               <sch:report test="count(child::tei:lem) eq 1 and not(child::tei:lem[@source]) and not(child::tei:rdg or child::tei:note)">When the apparatus entry contains only a lem, the @source is
                                    mandatory</sch:report>
            </sch:rule>
         </pattern>
         <attribute name="loc">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(location) indicates the location of the variation, when the location-referenced method of apparatus markup is used.</a:documentation>
            <list>
               <oneOrMore>
                  <data type="token">
                     <param name="pattern">(\p{L}|\p{N}|\p{P}|\p{S})+</param>
                  </data>
               </oneOrMore>
            </list>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="listApp">
      <element name="listApp">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(list of apparatus entries) contains a list of apparatus entries. [12.2. Linking the Apparatus to the Text]</a:documentation>
         <oneOrMore>
            <ref name="app"/>
         </oneOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="dharma-listApp-div-constraint-rule-63">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xs="http://www.w3.org/2001/XMLSchema"
                      context="tei:listApp">
               <sch:assert test="ancestor::tei:div[@type='apparatus']">ListApp can only be
                                    used inside the apparatus.</sch:assert>
            </sch:rule>
         </pattern>
      </element>
   </define>
   <define name="lem">
      <element name="lem">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(lemma) contains the lemma, or base text, of a textual variation. [12.1. The Apparatus Entry, Readings, and Witnesses]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="model.divLike"/>
               <ref name="model.divPart"/>
               <ref name="model.gLike"/>
               <ref name="model.phrase"/>
               <ref name="model.inter"/>
               <ref name="model.global"/>
               <ref name="model.rdgPart"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="dharma-lem-exclusion-inside-lem-constraint-rule-64">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xs="http://www.w3.org/2001/XMLSchema"
                      context="tei:lem">
               <sch:report test="child::tei:p">The
                                    element p is not allowed in lem</sch:report>
               <sch:report test="child::tei:ab">The element ab is not allowed in
                                    lem</sch:report>
               <sch:report test="child::tei:lg">The element lg
                                    is not allowed in lem</sch:report>
               <sch:report test="count(child::tei:l) eq 1">Don't keep the l element in lem
                                    if you have only one of
                            them</sch:report>
            </sch:rule>
         </pattern>
         <ref name="att.global.source.attribute.source"/>
         <empty/>
      </element>
   </define>
   <define name="rdg">
      <element name="rdg">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(reading) contains a single reading within a textual variation. [12.1. The Apparatus Entry, Readings, and Witnesses]</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="model.divLike"/>
               <ref name="model.divPart"/>
               <ref name="model.gLike"/>
               <ref name="model.phrase"/>
               <ref name="model.inter"/>
               <ref name="model.global"/>
               <ref name="model.rdgPart"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="dharma-rdg-exclusion-inside-lem-constraint-rule-65">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xs="http://www.w3.org/2001/XMLSchema"
                      context="tei:rdg">
               <sch:report test="child::tei:p">The
                                    element p is not allowed in rdg</sch:report>
               <sch:report test="child::tei:ab">The element ab is not allowed in
                                    rdg</sch:report>
               <sch:report test="child::tei:lg">The element lg
                                    is not allowed in rdg</sch:report>
               <sch:report test="child::tei:l">The element l is not allowed in
                                    rdg</sch:report>
            </sch:rule>
         </pattern>
         <attribute name="source">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifies the source from which some aspect of this element is drawn.</a:documentation>
            <list>
               <oneOrMore>
                  <data type="anyURI"/>
               </oneOrMore>
            </list>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="model.global.spoken">
      <notAllowed/>
   </define>
   <define name="model.divPart.spoken">
      <notAllowed/>
   </define>
   <define name="model.divPart.spoken_alternation">
      <notAllowed/>
   </define>
   <define name="model.divPart.spoken_sequence">
      <empty/>
   </define>
   <define name="model.divPart.spoken_sequenceOptional">
      <empty/>
   </define>
   <define name="model.divPart.spoken_sequenceOptionalRepeatable">
      <empty/>
   </define>
   <define name="model.divPart.spoken_sequenceRepeatable">
      <notAllowed/>
   </define>
   <define name="model.entryLike">
      <notAllowed/>
   </define>
   <define name="model.ptrLike.form">
      <notAllowed/>
