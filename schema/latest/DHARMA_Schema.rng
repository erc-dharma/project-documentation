   <define name="num">
      <element name="num">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(number) contains a number, written in any form. [3.5.3. Numbers and
Measures]</a:documentation>
         <ref name="macro.phraseSeq"/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="dharma-num-valeur-or-interval-constraint-rule-30">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xs="http://www.w3.org/2001/XMLSchema"
                      context="tei:num">
               <sch:report test="@value and (@atMost or @atLeast)">@value can not be used
                                    with the attributes of date range</sch:report>
               <sch:report test="@atMost and not(@atLeast)">@atLeast is expected with
                                    @atMost</sch:report>
               <sch:report test="@atLeast and not(@atMost)">@atMost is expected with
                            @atLeast</sch:report>
            </sch:rule>
         </pattern>
         <ref name="att.global.responsibility.attribute.cert"/>
         <ref name="att.ranging.attribute.atLeast"/>
         <ref name="att.ranging.attribute.atMost"/>
         <optional>
            <attribute name="value">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">supplies the value of the number in standard form.</a:documentation>
               <choice>
                  <data type="double"/>
                  <data type="token">
                     <param name="pattern">(\-?[\d]+/\-?[\d]+)</param>
                  </data>
                  <data type="decimal"/>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="measure">
