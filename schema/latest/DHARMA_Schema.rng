            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      context="tei:certainty">
               <sch:assert test="@match='../@met' and @locus='value'"> @match="../@met" and
                            @locus="value" always work together. </sch:assert>
